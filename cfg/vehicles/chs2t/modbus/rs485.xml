<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <SerialPort>
        <!-- <Name>/dev/rs485</Name> -->
        <Name>/dev/rs485</Name>
        <Baudrate>115200</Baudrate>        
        <DataBits>8</DataBits>
        <StopBits>1</StopBits>
        <Parity>0</Parity>
        <Timeout>50</Timeout>
        <Retries>1</Retries>
        <RequestDelay>100</RequestDelay>
        <ModbusMap>modbus-map</ModbusMap>
    </SerialPort>   
</Config>
